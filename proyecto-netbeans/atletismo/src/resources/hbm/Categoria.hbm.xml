<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="ar.com.atletismo.dominio.Categoria" table="Categoria">
        <id name="id" column="id_categoria">
            <generator class="increment"/>
        </id>
        <property name="descripcion" column="descripcion" type="string" />
        <property name="sexo" column="sexo" type="string" />
        <property name="edadDesde" column="edad_desde" type="integer" />
        <property name="edadHasta" column="edad_hasta" type="integer" />
  </class>
</hibernate-mapping>
